<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="Dev" class="org.lifehive.Dev">
        <property name="objNo" value="6"/>
        <property name="laptop" ref="Laptop"/>
    </bean>
    <bean id="Dev2" class="org.lifehive.Dev">
        <constructor-arg index="0" value="129"/>
        <constructor-arg index="1" ref="Laptop"/>
    </bean>
    <bean id="Laptop" class="org.lifehive.Laptop"/>
</beans>